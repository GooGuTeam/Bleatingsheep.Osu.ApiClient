<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;netstandard2.0;net46;net8.0</TargetFrameworks>
    <Version>0.6.21</Version>
    <Authors>bleatingsheep</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>bleatingsheep</Copyright>
    <Description>osu! API Client.</Description>
    <PackageTags>osu</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="WebApiClient.AOT" Version="1.1.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bleatingsheep.Osu.Enums\Bleatingsheep.Osu.Enums\Bleatingsheep.Osu.Enums.csproj" />
  </ItemGroup>

</Project>
